{
  "name": "fpl_bot",
  "version": "1.0.0",
  "description": "Analyse Fantasy Premier League stats",
  "main": "./lib/index.js",
  "bin": {
    "fpl-cli": "./bin/fpl-cli"
  },
  "scripts": {
    "start": "npm run build && npm run global",
    "build": "npm run clean:some && tsc -p .",
    "global": "npm i -g && fpl-cli",
    "clean:some": "rm -rf ./lib",
    "clean:all": "rm -rf ./node_modules ./package-lock.json ./lib",
    "refresh": "npm run clean:all && npm install"
  },
  "author": "Anthony Pengelly",
  "license": "ISC",
  "dependencies": {
    "tslib": "^2.0.0",
    "web-request": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^14.0.13",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  }
}
